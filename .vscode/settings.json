{
  "editor.tabSize": 2,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.next/*/**": true
  },
  "search.exclude": {
    "**/node_modules/*/**": true,
    "**/bower_components": true,
    "**/*.code-search": true
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.vscode": true,
    "**/build": true,
    "**/package-lock.json": true
  },
  "explorerExclude.backup": null
}